<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <Button
        android:id="@+id/browse_items_view_order_button"
        android:layout_width="173dp"
        android:layout_height="56dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="12dp"
        android:text="@string/View_Order"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/browse_items_back_button"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/browse_items_back_button"
        android:layout_width="129dp"
        android:layout_height="54dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="12dp"
        android:text="@string/Back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/browse_items_recycler"
        android:layout_width="370dp"
        android:layout_height="581dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.609"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/browse_items_view_order_button" />

</androidx.constraintlayout.widget.ConstraintLayout>